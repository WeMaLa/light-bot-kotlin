  "main": "index.js",
  "scripts": {
    "clean:cache": "npm cache clean",
    "clean:node-modules": "rimraf node_modules",
    "clean:all": "npm cache verify && rimraf node_modules",
    "install:node-modules": "npm install",
    "app:clean": "rimraf package-lock.json && rimraf ../src/main/resources/static/app/ && rimraf lint-reports",
    "app:watch": "npm run app:clean && webpack --env=dev --progress --profile --colors --display-error-details --display-cached --watch",
    "app:build": "npm run app:clean && webpack --env=prod --progress --profile --colors --display-error-details --display-cached"
  },
