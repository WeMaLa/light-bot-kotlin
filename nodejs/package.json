    "sockjs-client": "1.1.4",
    "@stomp/stompjs": "4.0.1"
  },
  "devDependencies": {
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "webpack-merge": "4.1.1",
    "ts-node": "~2.0.0",
